    "#write a program of sum of n given number using while loop\n",
    "n=int(input(\"enter the number:\"))\n",
    "sum=0\n",
    "i=0\n",
    "while i<=n:\n",
    "    sum+=i\n",
    "    i+=1\n",
    "    print(i)\n",
    "print(sum)"
